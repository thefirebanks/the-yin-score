// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Node {
  id   Int    @id @default(autoincrement())
  name String @unique
  Link Link[]
}

model Link {
  id     Int    @id @default(autoincrement())
  source String
  target String
  node   Node   @relation(fields: [source], references: [name])
}
